<!DOCTYPE html>
<html>

<head>
  <meta charset="utf-8">
</head>

<h1>Log History:</h1>

<body>
  <ul>
    <% data.forEach(function(dat) { %>
        User: <%= dat.user %>
        <br>
        Message: <%= dat.message%>
        <br>
        Date: <%= dat.time%>
        <br>
        <br>
    <% }); %>
  </ul>
 </body>

</html>